{
  "name": "circuit-sim",
  "version": "1.1.2",
  "description": "A digital circuit simulator using React, TypeScript and p5.js",
  "homepage": "http://hasnainroopawalla.github.io/circuit-sim",
  "private": true,
  "scripts": {
    "start": "npm run start --workspace @circuit-sim/web",
    "start:prod": "npm run start:prod --workspace @circuit-sim/web",
    "build": "npm run build --workspace @circuit-sim/web",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest --watch",
    "test:unit": "jest --coverage",
    "lint": "biome check",
    "lint:fix": "biome check --fix . && biome format --write .",
    "stats": "npm run stats --workspace @circuit-sim/web"
  },
  "keywords": [
    "react",
    "typescript",
    "p5js",
    "circuit-simulator",
    "javascript",
    "webpack",
    "digital-logic-design"
  ],
  "author": "Hasnain Roopawalla",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@biomejs/biome": "2.2.5",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/p5": "^1.7.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "css-loader": "^6.8.1",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.6.2",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-mxn": "^0.0.6",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "p5": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
